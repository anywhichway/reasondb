{
  "name": "reasondb",
  "version": "0.2.7",
  "description": "A 100% JavaScript browser/nodejs object database with SQL like query syntax and swapable persistence engines, plus streaming analytics, 18 built-in predicates, in-line fat arrow predicates, predicate extensibility, indexed Dates and Arrays, joins, nested matching in as little as 70K.",
  "main": "lib/index.js",
  "client": "./browser/reasondb.js",
  "reveal": true,
  "isomorphic": false,
  "scripts": {
    "test": "istanbul cover node_modules/mocha/bin/_mocha --report lcov -- -R spec",
    "build": "babel src -d lib && cp src/LocalStorage.js lib/LocalStorage.js && npm run browserify",
    "browserify": "browserify lib/index.js -t babelify  -o browser/reasondb.js",
    "prepublish": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/anywhichway/reasondb.git"
  },
  "keywords": [
    "CouchDB",
    "PouchDB",
    "Firebase",
    "MongoDB",
    "Cloudbase",
    "CloudDB",
    "JOQULAR",
    "JSON",
    "database",
    "Realm",
    "Redis",
    "Memcached",
    "IronCache",
    "levelup",
    "leveldown",
    "Localstorage"
  ],
  "author": "Simon Y. Blackwell <syblackwell@anywhichway.com> (http://www.github.com/anywhichway)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/anywhichway/reasondb/issues"
  },
  "homepage": "https://github.com/anywhichway/reasondbl#readme",
  "devDependencies": {
    "babel-cli": "^6.16.0",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-latest": "^6.16.0",
    "babelify": "^7.3.0",
    "blanket": "^1.2.3",
    "bluebird": "^3.3.5",
    "browserify": "^13.0.0",
    "chai": "^3.4.1",
    "codeclimate-test-reporter": "^0.2.0",
    "iron-cache": "^0.3.0",
    "istanbul": "^0.4.2",
    "leveldown": "^1.5.0",
    "levelup": "^1.3.3",
    "localforage": "^1.4.3",
    "memjs": "^0.10.0",
    "mocha": "^2.3.4",
    "redis": "^2.6.2"
  },
  "dependencies": {
    "babel-runtime": "^6.11.6",
    "graceful-fs": "^4.1.9",
    "localforage": "^1.4.3",
    "uuid": "^3.0.0",
    "write-file-atomic": "^1.2.0"
  }
}
